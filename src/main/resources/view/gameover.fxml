<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml" fx:id="rootgameover" prefHeight="896.0" prefWidth="1600.0"
            styleClass="backgameover" stylesheets="@style/stylesheet.css" xmlns="http://javafx.com/javafx"
            fx:controller="fr.dashstrom.controller.GameOverController">
    <top>
        <Label fx:id="gameovertext" alignment="CENTER" prefHeight="267.0" prefWidth="562.0" styleClass="textgameover"
               stylesheets="@style/stylesheet.css" text="GAME OVER" textAlignment="CENTER"
               BorderPane.alignment="CENTER"/>
    </top>
    <bottom>
        <Button fx:id="quitbutton" mnemonicParsing="false" onMouseClicked="#quit" prefHeight="84.0" prefWidth="308.0"
                styleClass="wood-button" text="QUITTER" textAlignment="CENTER" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="300.0"/>
            </BorderPane.margin>
        </Button>
    </bottom>
    <center>
        <Button fx:id="replaybutton" mnemonicParsing="false" onMouseClicked="#replay" prefHeight="84.0"
                prefWidth="308.0" styleClass="wood-button" text="REJOUER" textAlignment="CENTER"
                BorderPane.alignment="CENTER"/>
    </center>
</BorderPane>
